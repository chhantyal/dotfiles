[alias]
  cm = commit -m
  cma = commit -am
  st = status
  co = checkout
  df = diff
  cl = clone
  lg = log
[user]
  name = Madhusudhan Srinivasa
  email = madhums8@gmail.com
[format]
  pretty = format:%Cred%h%Creset %C(yellow)%an%d%Creset %s %Cgreen(%cr)%Creset
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
